    "def prac1(arr):\n",
    "    if len(set(arr)) != len(arr):\n",
    "        return False\n",
    "    else:\n",
    "        for i in range(len(arr)):\n",
    "            if arr[i] != i+1:\n",
    "                return False\n",
    "        return True\n",
    "\n",
    "arr = [4,1,2,3]\n",
    "arr.sort()\n",
    "\n",
    "print(prac1(arr))\n",
    "\n"
   ]

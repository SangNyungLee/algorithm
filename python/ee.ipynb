    "def fnCalculation(x,y):\n",
    "    result = 0\n",
    "    for i in range(len(x)):\n",
    "        temp = x[i : i + len(y)]\n",
    "        if temp == y:\n",
    "            result += 1\n",
    "    return result\n",
    "\n",
    "a = \"abdcabcabca\"\n",
    "p1 = \"ab\"\n",
    "p2 = \"ca\"\n",
    "\n",
    "out = f\"ab{fnCalculation(a, p1)}ca{fnCalculation(a, p2)}\"\n",
    "print(out)"
   ]

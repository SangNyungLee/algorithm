    "def solution(arr, flag):\n",
    "    answer = []\n",
    "    for idx, a in enumerate(flag):\n",
    "        if a:\n",
    "            for i in range(arr[idx]*2):\n",
    "                answer.append(arr[idx])\n",
    "        else:\n",
    "            for i in range(arr[idx]):\n",
    "                answer.pop()\n",
    "    return answer"
   ]
  }

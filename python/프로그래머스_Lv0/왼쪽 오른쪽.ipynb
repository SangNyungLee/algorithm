    "def solution(str_list):\n",
    "    answer = []\n",
    "    \n",
    "    for i, j in enumerate(str_list):\n",
    "        if j == \"l\":\n",
    "            answer = str_list[:i]\n",
    "            break\n",
    "        elif j == \"r\":\n",
    "            answer = str_list[i+1:]\n",
    "            break\n",
    "    return answer"
   ]
  }

    "def solution(num_list):\n",
    "    answer = 0\n",
    "    for i in num_list:\n",
    "        while i!=1:\n",
    "            if i % 2 == 0:\n",
    "                i/=2\n",
    "                answer +=1\n",
    "            elif i % 2 == 1:\n",
    "                i = (i-1)/2\n",
    "                answer+=1\n",
    "    return answer"
   ]

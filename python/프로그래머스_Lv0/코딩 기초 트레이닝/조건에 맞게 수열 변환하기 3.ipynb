    "def solution(arr, k):\n",
    "    answer = []\n",
    "    if k % 2 == 1:\n",
    "        for i in arr:\n",
    "            answer.append(i*k)\n",
    "    else:\n",
    "        for i in arr:\n",
    "            answer.append(i+k)\n",
    "    return answer"
   ]

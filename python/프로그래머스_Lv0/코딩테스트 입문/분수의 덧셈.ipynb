    "import math\n",
    "def solution(numer1, denom1, numer2, denom2):\n",
    "    numer = numer1*denom2 + numer2*denom1\n",
    "    denom = denom1 * denom2\n",
    "    gcd = math.gcd(numer,denom)\n",
    "    \n",
    "    return numer/gcd, denom/gcd"
   ]
  }

    "def solution(num_list):\n",
    "    answer = num_list\n",
    "    res=0\n",
    "    i = len(num_list)-1\n",
    "    if num_list[i] > num_list[i-1]:\n",
    "        res = num_list[i]-num_list[i-1]\n",
    "    else:\n",
    "        res = num_list[i]*2\n",
    "        \n",
    "    answer.append(res)\n",
    "    \n",
    "    return answer"
   ]

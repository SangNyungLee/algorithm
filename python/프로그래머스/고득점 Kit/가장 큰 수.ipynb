    "def solution(numbers):\n",
    "    str_list = [str(item) for item in numbers]\n",
    "    str_list.sort(key=lambda x : x*3, reverse=True)\n",
    "    if(int(''.join(str_list)) == 0 ):\n",
    "        return \"0\"\n",
    "    else:\n",
    "        return ''.join(str_list)"
   ]

    "import math\n",
    "n = int(input())\n",
    "arr = []\n",
    "a = int(input())\n",
    "for i in range(n-1):\n",
    "    num = int(input())\n",
    "    arr.append(num-a)\n",
    "    a = num\n",
    "\n",
    "g = arr[0]\n",
    "for j in range(1, len(arr)):\n",
    "    g = math.gcd(g, arr[j])\n",
    "result = 0\n",
    "for i in arr:\n",
    "    result += i//g-1\n",
    "print(result)"
   ]
  }

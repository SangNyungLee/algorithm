   "source": [
    "n = int(input())\n",
    "\n",
    "for i in range(n):\n",
    "    sum = 0\n",
    "    vps = input()\n",
    "    for i in vps:\n",
    "        if i == \"(\":\n",
    "            sum +=1\n",
    "        elif i == \")\":\n",
    "            sum -=1\n",
    "        if sum == -1 :\n",
    "            break\n",
    "    if sum == 0:\n",
    "        print(\"YES\")\n",
    "    else:\n",
    "        print(\"NO\")"
   ]
  }

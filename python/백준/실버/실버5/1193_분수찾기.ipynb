   "source": [
    "#어떠한 값을 입력받았을 때 그게 몇번 째 줄에 있고 어디 위치에 있는지 확인\n",
    "n = int(input())\n",
    "num = 1\n",
    "hap = 1\n",
    "while(True):\n",
    "    if(num >= n):\n",
    "        break\n",
    "    hap+=1\n",
    "    num += hap\n",
    "k = num-n\n",
    "if hap%2 == 0:\n",
    "    print(hap-k, end=\"/\")\n",
    "    print(1+k)\n",
    "\n",
    "if hap%2 != 0:\n",
    "    print(1+k, end=\"/\")\n",
    "    print(hap-k)"
   ]
  }
 ],

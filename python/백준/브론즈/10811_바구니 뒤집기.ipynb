    "n,m = map(int, input().split())\n",
    "\n",
    "arr = [i for i in range(1,n+1)]\n",
    "for i in range(m):\n",
    "    a,b = map(int, input().split())\n",
    "    newArr = arr[a-1:b]\n",
    "    newArr.reverse()\n",
    "    arr[a-1:b] = newArr\n",
    "for i in arr:\n",
    "    print(i, end=' ')"
   ]

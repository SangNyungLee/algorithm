   "source": [
    "exp = input().split('-')\n",
    "num = []\n",
    "\n",
    "for i in exp:\n",
    "    sum = 0\n",
    "    tmp = i.split('+')\n",
    "    for j in tmp:\n",
    "        sum += int(j)\n",
    "    num.append(sum)\n",
    "\n",
    "n = num[0]\n",
    "\n",
    "for i in range(1, len(num)):\n",
    "    n -= num[i]\n",
    "print(n)"
   ]
  }

    "from bisect import bisect_left, bisect_right\n",
    "\n",
    "N, x = map(int, input().split(' '))\n",
    "array = list(map(int, input().split()))\n",
    "count = bisect_right(array, x) - bisect_left(array, x)\n",
    "if count == 0:\n",
    "    print(-1)\n",
    "else:\n",
    "    print(count)"
   ]
